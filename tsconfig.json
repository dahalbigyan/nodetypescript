{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "pretty": true,
    "sourceMap": true,
    "target": "es6",
    "outDir": "dist",
    "baseUrl": "lib"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
